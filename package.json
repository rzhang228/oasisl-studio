{
  "name": "oasisl-studio",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build-renderer-dev": "webpack --progress --watch --hide-modules --config ./build/webpack.renderer.dev.conf",
    "build-main-dev": "webpack --progress --watch --hide-modules --config ./build/webpack.main.base.conf",
    "build-renderer-prod": "webpack --progress --hide-modules --config ./build/webpack.renderer.prod.conf",
    "build-main-prod": "webpack --progress --hide-modules --config ./build/webpack.main.prod.conf",
    "start-prod": "cross-env NODE_ENV=production electron .",
    "build-dev": "npm-run-all --parallel build-main-dev build-renderer-dev",
    "start-dev": "cross-env NODE_ENV=development node main.dev",
    "start": "npm-run-all build-main-prod build-renderer-prod start-prod"
  },
  "dependencies": {
    "electron": "^1.7.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chokidar": "^2.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "electron-connect": "^0.6.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
