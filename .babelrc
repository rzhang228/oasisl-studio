//babel配置文件，不需要做修改，因为都配置好了
{
  "presets": [
    [
      "env",
      {
        "targets": {
          "chrome": 58
        }
      }
    ],
    "react",
    "stage-0",
    "stage-1",
    "stage-2",
    "stage-3"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    // "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-async-to-generator",
    "transform-do-expressions",
    "syntax-do-expressions"
  ],
  "comments": false
}